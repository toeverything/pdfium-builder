{
  "name": "@toeverything/pdf-viewer",
  "version": "0.1.0",
  "author": "toeverything",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/toeverything/pdfium-builder.git",
    "directory": "packages/pdf-viewer"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "src",
    "dist",
    "!src/__tests__",
    "!dist/__tests__"
  ],
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "scripts": {
    "build": "tsc -b -v",
    "test": "vitest --run",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@toeverything/pdf-viewer-types": "workspace:*",
    "@toeverything/pdfium": "workspace:*"
  },
  "devDependencies": {
    "@toeverything/resources": "workspace:*",
    "@vitest/ui": "2.1.3",
    "happy-dom": "^15.7.4",
    "vitest": "^2.1.3"
  }
}
