{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": false,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "composite": false
  },
  "include": ["./index.ts"],
  "references": [
    {
      "path": "../pdf-viewer"
    },
    {
      "path": "../pdfium"
    },
    {
      "path": "../types"
    }
  ]
}
